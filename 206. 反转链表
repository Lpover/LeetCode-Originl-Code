/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode() : val(0), next(nullptr) {}
 *     ListNode(int x) : val(x), next(nullptr) {}
 *     ListNode(int x, ListNode *next) : val(x), next(next) {}
 * };
 */
class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        ListNode* ans=NULL;
        ListNode* cur=NULL;
        ListNode* pre=head;
        while(pre!=NULL){
            ListNode* node=pre->next;
            pre->next=cur;
            cur=pre;
            pre=node;
        }
        return cur;
    }
};
